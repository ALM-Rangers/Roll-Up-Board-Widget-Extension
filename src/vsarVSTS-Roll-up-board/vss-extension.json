{
  "manifestVersion": 1,
  "id": "RollUpBoardWidgetTest",
  "version": "3.8.71",
  "name": "RollUp Board Test",
  "publisher": "DevMikaelKrief",
  "scopes": [
    "vso.work"
  ],
  "description": "The widget shows the number of cards in each column of the Kanban board.",
  "icons": {
    "default": "static/images/logo.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services.Cloud"
    },
    {
      "id": "Microsoft.TeamFoundation.Server",
      "version": "[15.0,)"
    }
  ],
  "tags": [
    "Board",
    "Widget"
  ],
  "links": {
    "repository": {
      "uri": "https://github.com/ALM-Rangers/Roll-Up-Board-Widget-Extension"
    },
    "issues": {
      "uri": "https://github.com/ALM-Rangers/Roll-Up-Board-Widget-Extension/issues"
    },
    "support": {
      "uri": "http://aka.ms/vsarcontact"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/ALM-Rangers/Roll-Up-Board-Widget-Extension"
  },
  "branding": {
    "color": "rgb(51,65,133)",
    "theme": "dark"
  },
  "content": {
    "license": {
      "path": "mp_terms.md"
    },
    "details": {
      "path": "overview.md"
    }
  },
  "files": [
    {
      "path": "overview.md",
      "addressable": true
    },
    {
      "path": "ThirdPartyNotices.txt",
      "addressable": true
    },
    {
      "path": "dist",
      "addressable": true
    },
    {
      "path": "static",
      "addressable": true
    },
    {
      "path": "node_modules/vss-web-extension-sdk/lib",
      "addressable": true,
      "packagePath": "lib"
    },
    {
      "path": "node_modules/applicationinsights-js/dist",
      "addressable": true,
      "packagePath": "lib"
    },
    {
      "path": "node_modules/ldclient-js/dist",
      "addressable": true,
      "packagePath": "lib"
    },
    {
      "path": "node_modules/event-source-polyfill/src",
      "addressable": true,
      "packagePath": "lib"
    }
  ],
  "categories": [
    "Plan and track"
  ],
  "contributions": [
    {
      "id": "rollupboardwidget",
      "type": "ms.vss-dashboards-web.widget",
      "targets": [
        "ms.vss-dashboards-web.widget-catalog",
        ".rollupboardwidget-Configuration"
      ],
      "properties": {
        "name": "Roll-Up Board",
        "description": "View a board summary, showing number of cards for both columns and swimlanes.",
        "uri": "static/index.html",
        "isNameConfigurable": true,
        "catalogIconUrl:": "static/images/logo.png",
        "previewImageUrl": "static/images/logo.png",
        "supportedSizes": [
          {
            "rowSpan": 1,
            "columnSpan": 3
          },
          {
            "rowSpan": 2,
            "columnSpan": 3
          },
          {
            "rowSpan": 3,
            "columnSpan": 3
          },
          {
            "rowSpan": 1,
            "columnSpan": 4
          },
          {
            "rowSpan": 2,
            "columnSpan": 4
          },
          {
            "rowSpan": 3,
            "columnSpan": 4
          },
          {
            "rowSpan": 4,
            "columnSpan": 4
          },
          {
            "rowSpan": 4,
            "columnSpan": 3
          }
        ],
        "supportedScopes": [
          "project_team"
        ]
      }
    },
    {
      "id": "rollupboardwidget-Configuration",
      "type": "ms.vss-dashboards-web.widget-configuration",
      "targets": [
        "ms.vss-dashboards-web.widget-configuration"
      ],
      "properties": {
        "name": "Roll-Up Board Configuration",
        "description": "Configure Roll-Up Board widget.",
        "uri": "static/configuration.html"
      }
    }
  ]
}