{
  "manifestVersion": 1,
  "id": "",
  "version": "",
  "name": "",
  "scopes": [ "vso.work" ],
  "description": "The widget shows the number of cards in each column of the Kanban board.",
  "publisher": "",
  "icons": {
    "default": "img/logo.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "tags": [
    "Board",
    "Widget"
  ],
  "links": {
    "support": {
      "uri": "http://aka.ms/vsarcontact"
    }
  },
  "branding": {
    "color": "rgb(51,65,133)",
    "theme": "dark"
  },
  "content": {
    "license": {
      "path": "mp_terms.md"
    },
    "details": {
      "path": "overview.md"
    }
  },
  "files": [
    {
      "path": "overview.md",
      "addressable": true
    },
    {
      "path": "css",
      "addressable": true
    },
    {
      "path": "img",
      "addressable": true
    },
    {
      "path": "scripts",
      "addressable": true
    },
    {
      "path": "index.html",
      "addressable": true
    },
    {
      "path": "configuration.html",
      "addressable": true
    },
    {
      "path": "ThirdPartyNotices.txt",
      "addressable": true
    }
  ],
  "categories": [
    "Plan and track"
  ],
  "contributions": [
    {
      "id": "rollupboardwidget",
      "type": "ms.vss-dashboards-web.widget",
      "targets": [
        "ms.vss-dashboards-web.widget-catalog",
        ".rollupboardwidget-Configuration"
      ],
      "properties": {
        "name": "Roll-Up Board",
        "description": "View board summary",
        "uri": "index.html",
        "isNameConfigurable": true,
        "previewImageUrl": "img/logo.png",
        "supportedSizes": [
          {
            "rowSpan": 1,
            "columnSpan": 3
          },
          {
            "rowSpan": 2,
            "columnSpan": 3
          },
          {
            "rowSpan": 3,
            "columnSpan": 3
          },
          {
            "rowSpan": 1,
            "columnSpan": 4
          },
          {
            "rowSpan": 2,
            "columnSpan": 4
          },
          {
            "rowSpan": 3,
            "columnSpan": 4
          }

        ],
        "supportedScopes": [ "project_team" ]
      }
    },
    {
      "id": "rollupboardwidget-Configuration",
      "type": "ms.vss-dashboards-web.widget-configuration",
      "targets": [ "ms.vss-dashboards-web.widget-configuration" ],
      "properties": {
        "name": "Roll-Up Board Widget Configuration",
        "description": "Configure Roll-Up Board Widget",
        "uri": "configuration.html"
      }
    }
  ]
}
